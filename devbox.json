{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.2/.schema/devbox.schema.json",
  "packages": [
    "python3@latest",
    "pipenv@latest",
    "terraform@latest"
  ],
  "shell": {
    "init_hook": [
      "typeset -g POWERLEVEL9K_INSTANT_PROMPT=off",
      "pipenv install",
      "alias cdktf='npx cdktf-cli@latest'"
    ],
    "scripts": {
      "test": [
        "echo \"Error: no test specified\" && exit 1"
      ]
    }
  }
}
